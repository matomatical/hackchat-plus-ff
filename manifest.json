{

  "manifest_version": 2,
  "name": "HackChat+",
  "version": "1.0",

  "description": "Adds some extra KaTeX functionality to Hack.Chat",

  "content_scripts": [
    {
      "matches": ["*://hack.chat/*"],
      "js": ["katex/katex.min.js", "katex/contrib/auto-render.min.js", "chathacks.js"]
    }
  ]

}